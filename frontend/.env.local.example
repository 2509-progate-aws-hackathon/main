# RDS Data API 直接アクセス用環境変数
# セキュリティを無視した設定例

# AWS基本設定
NEXT_PUBLIC_AWS_REGION=us-east-1

# Aurora Serverless 設定
NEXT_PUBLIC_AURORA_CLUSTER_ARN=arn:aws:rds:us-east-1:YOUR_ACCOUNT_ID:cluster:jiko-database
NEXT_PUBLIC_AURORA_SECRET_ARN=arn:aws:secretsmanager:us-east-1:YOUR_ACCOUNT_ID:secret:jiko-database-secret
NEXT_PUBLIC_AURORA_DATABASE_NAME=postgres

# AWS認証情報 (本来はセキュリティ上推奨されない)
# NEXT_PUBLIC_AWS_ACCESS_KEY_ID=AKIA...
# NEXT_PUBLIC_AWS_SECRET_ACCESS_KEY=...

# Location Service (既存)
NEXT_PUBLIC_MAP_NAME=amplify-map